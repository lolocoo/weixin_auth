        <link rel="stylesheet" href="../static/css/main.css">
        <script src="../static/js/vendor/modernizr-2.8.3.min.js"></script>
        <style>
            html, body, .page, .intro-item {
                width: 100%;
                height: 100%;
            }
            .page {
                display: none;
                overflow: hidden;
               
            }
            .hide {
                display: none;
            }
            #animateTest img {
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                display: none;
            }
            #animateTest img.show,
            #animateTest img:first-child {
                display: block;
            }
            .intro-item {
                position: absolute;
                top: 0;
                left: 0;
            }
            .intro-bg {
                width: 100%;
                height: 100%;
                /*background: url(../static/img/smailday/sky_0.jpg) center top no-repeat;*/
                background-size: 100% auto;
                z-index: 10;
                -webkit-animation-duration: .3s;
            }
            .intro-bg-main {
                z-index: 11;
            }
            .intro-building {
                width: 100%;
                height: 100%;
                /*background: url(../static/img/smailday/building_0.png) center top no-repeat;*/
                background-size: 100% auto;
                z-index: 20;
                -webkit-animation-delay: .2s;
            }
            .intro-earth {
                width: 100%;
                height: 100%;
                /*background: url(../static/img/smailday/earth_0.png) center top no-repeat;*/
                background-size: 100% auto;
                z-index: 30;
                -webkit-animation-delay: .5s;
            }
            .intro-building-main {
                z-index: 40;
                -webkit-animation-duration: .8s;
            }
        </style>
    </head>
